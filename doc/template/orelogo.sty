    \begin{flushright}%
    {\Huge ORE}\par%
    {\Large Open Rating Environment}
    \end{flushright}
    }}
    & 
    \includegraphics[width=0.2\textwidth]{../logo/ore.eps} \\ 
    \end{tabular}
    \vskip 1.5em%
    {\small \@date \par}%       % Set date in \large size.
  \end{flushright}\par
  \@thanks
  \vfil\null
  \end{titlepage}%
  \setcounter{footnote}{0}%
  \global\let\thanks\relax
  \global\let\maketitle\relax
  \global\let\@thanks\@empty
  \global\let\@author\@empty
  \global\let\@date\@empty
