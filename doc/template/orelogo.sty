  \let \footnote \thanks
  \null\vfil
  \vskip 40\p@
  \begin{center}%
    %\includegraphics[width=0.3\textwidth]{ore}
    {\Huge \@title \par}%
    \vskip 12em%
  \end{center}\par
  \begin{flushright}%
    {\large
     \lineskip .75em%
      \begin{tabular}[t]{@{} r @{}}%
        \@author
      \end{tabular}\par}%
  \end{flushright}
  \begin{center}%
    \setlength{\unitlength}{0.01\textwidth}
    \begin{picture}(100,7)
    	\linethickness{3mm}
	\put(0,6){\line(1,0){100}}
